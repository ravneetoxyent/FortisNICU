function calculateaddition() {
    var sum = 0;

    s(".sum").each(function() {

        if(!isNaN(this.value) && this.value.length!=0) {
            sum += parseFloat(this.value);
        }

    });
    

    s("#sumvalue").val(sum.toFixed(2));

}

