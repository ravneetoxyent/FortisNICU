<script>
function checkbirthweight()
{
    var birthwt= document.getElementById('birthwt').value;
 	if(birthwt.length < 3)
		{
			alert ("Please Enter Birth Weight Again. The weight should be in grams");
			document.getElementById('birthwt').focus();
		}
	
}	
	
function checkpresentweight()
{
    var presentwt = document.getElementById('presentwt').value;
	var yesterdaywt =  document.getElementById('yesterdaywt').value;    
	if(presentwt.length < 3)
	{
		alert ("Please Enter Present Weight Again. The weight should be in grams");
		document.getElementById('presentwt').focus();
	}
    var diffyesterdaywt = presentwt - yesterdaywt;

    var percentage1 = parseFloat(presentwt*10);
    var percentage2 = parseFloat(percentage1/100);
    var percentage3 = parseFloat(presentwt) + parseFloat(percentage2);
    
    var diffpercentage = parseFloat(percentage3)-parseFloat(presentwt);

    if(diffyesterdaywt > diffpercentage)
    {
    	alert ("Please insert present weight again. The increment shouldn't be more than 10 %");
    	return false;
    }
}

function checkyesterdayweight()
{
	var yesterdaywt =  document.getElementById('yesterdaywt').value;
	if(yesterdaywt.length < 3)
	{
		alert ("Please Enter Yesterday Weight Again. The weight should be in grams");
		document.getElementById('yesterdaywt').focus();
	}
}

function calculateSum() {
	
    var presentwt = document.getElementById('presentwt').value;
    var birthwt= document.getElementById('birthwt').value;
    var yesterdaywt =  document.getElementById('yesterdaywt').value;
    if(presentwt == '' || presentwt == 'NaN')
    	presentwt = 0;
    if(birthwt == '' || birthwt == 'NaN')
    	birthwt = 0;
    if(yesterdaywt == '' || yesterdaywt == 'NaN')
    	yesterdaywt = 0;
    
    var diffbirthwt = presentwt - birthwt;
        
    var diffyesterdaywt = presentwt - yesterdaywt;

    if(presentwt !='0' && birthwt !='0')
    {
    document.getElementById('diffbirthwt').value=diffbirthwt.toFixed(0);
	    if(diffbirthwt > 0)
		    {
		    	document.getElementById('from_birth').innerHTML= 'From Birth Weight Increment';
		    }
	    else if(diffbirthwt < 0)
		    {
		    	document.getElementById('from_birth').innerHTML= 'From Birth Weight Decrement';
		    }    
	    else
		    {
		    	document.getElementById('from_birth').innerHTML= 'From Birth Weight';
		    }
    }
    
    if(yesterdaywt!='0' && presentwt !='0')
    {
    document.getElementById('diffyesterdaywt').value=diffyesterdaywt.toFixed(0);
	    if(diffyesterdaywt > 0)
		    {
		    	document.getElementById('from_yesterday').innerHTML= 'From Yesterday Weight Increment';
		    }
	    else if(diffyesterdaywt < 0)
		    {
		    	document.getElementById('from_yesterday').innerHTML= 'From Yesterday Weight Decrement';
		    }    
	    else
		    {
		    	document.getElementById('from_yesterday').innerHTML= 'From Yesterday Weight';
		    }        
    }
}
</script>

<script>
function urine()
{

	var presentwt = document.getElementById('presentwt').value;
	presentwt = presentwt/1000;
	if(presentwt == '')
	{
		alert("Please enter the weight first");
		document.getElementById("presentwt").focus();
	}
	else
	{
    var init= 1;
    var max= 4; 
    var sum= document.getElementById('urine'+init).value;
	if(sum=='' || sum=='NaN')
	{
		sum = 0;
	}
    for(i = init+1; i<=max; i++)
    {
    	hour = (i)-init;
    	var urinevol = document.getElementById('urine'+i).value;
    	if(urinevol=='' || urinevol=='NaN')
    	{
    		urinevol = 0;
    	}    	
    	sum = eval(parseInt(sum) + parseInt(urinevol));
    	var totalurine = sum/presentwt/hour;
    	if(sum=='' || sum=='NaN')
    	{
    		sum = 0;
    	}
    	document.getElementById('totalurine' + i).value = totalurine.toFixed(1);
    }
}
}
</script>
